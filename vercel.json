{
  "version": 2,
  "buildCommand": "pnpm build && rm -rf .vercel/output && cp -R apps/web/.vercel/output .vercel/output",
  "framework": null,
  "regions": ["iad1"],
  "functions": {
    "_render": {
      "runtime": "nodejs20.x",
      "maxDuration": 30,
      "memory": 1024
    },
    "api/**": {
      "runtime": "python3.11",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "NODE_VERSION": "20"
  }
}
